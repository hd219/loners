<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!--<property name="addRemarkComments" value="true"/>--> <!-- useless -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection connectionURL="jdbc:mysql://192.168.33.6:3306/test"
                        driverClass="com.mysql.cj.jdbc.Driver"
                        userId="root" password="root">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.example.demo.practice.model.pojo"
                            targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.generator"
                         targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="com.example.demo.practice.model.mapper"
                             type="XMLMAPPER"
                             targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!-- DB tables to object  -->
        <!--<table tableName="test" domainObjectName="Test"/>-->
        <!--<table tableName="tags" domainObjectName="Tags"/>-->
        <!--<table tableName="course_tag" domainObjectName="CourseTag"/>-->
        <!--        <table tableName="account_config" domainObjectName="AccountConfig"/>-->
        <!--        <table tableName="wx_user_account" domainObjectName="WxUserAccount"/>-->
        <!--        <table tableName="wx_scene" domainObjectName="WxScene"/>-->
        <!--        <table tableName="user_info" domainObjectName="UserInfo"/>-->
        <table tableName="user_info" domainObjectName="UserInfo"/>
    </context>

</generatorConfiguration>
